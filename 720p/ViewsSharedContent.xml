<?xml version="1.0"?>
<includes>
	<include name="SharedContentMainBG">
		<include>BGSharedContent</include>
	</include>
	<include name="SharedContentLeafs">
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>264</width>
			<height>264</height>
			<include>Shadow</include>
		</control>
		<control type="image">
			<posx>13</posx>
			<posy>13</posy>
			<width>238</width>
			<height>238</height>
			<texture background="true">default-square-big.png</texture>
			<aspectratio scalediffuse="false" align="center" aligny="middle">scale</aspectratio>
		</control>
		<control type="image">
			<posx>13</posx>
			<posy>13</posy>
			<width>238</width>
			<height>238</height>
			<texture background="true">$INFO[ListItem.CompositeImage(width=720;height=720;cols=2;rows=2)]</texture>
			<aspectratio scalediffuse="false" align="center" aligny="middle">scale</aspectratio>
		</control>
	</include>
	<include name="SharedContentLeafsSelectedBorder">
		<control type="image">
			<width>244</width>
			<height>244</height>
			<include>Border</include>
		</control>
	</include>
	<include name="SharedContentLeafsTitle">
		<posy>258</posy>
		<posx>134</posx>
		<width>238</width>
		<height>30</height>
		<align>center</align>
		<aligny>center</aligny>
		<label>$INFO[ListItem.Label]</label>
		<font>LightCaps-26</font>
		<shadowcolor>FF222222</shadowcolor>
	</include>
	<include name="SharedContentLeafsTitle2">
		<posy>288</posy>
		<posx>134</posx>
		<width>238</width>
		<height>30</height>
		<align>center</align>
		<aligny>center</aligny>
		<label>$INFO[ListItem.Property(serverOwner)] &#xB7; $INFO[ListItem.Property(serverName)]</label>
		<textcolor>Color3</textcolor>
		<font>Regular-17</font>
		<shadowcolor>FF222222</shadowcolor>
	</include>	
	<include name="SharedContent">
		<control type="group">
			<control type="group">
				<visible>Control.IsVisible(54)</visible>
				<visible>!Skin.String(54_Dim,100)</visible>
				<include>SharedContentMainBG</include>
			</control>
			<!-- main list -->
			<control type="group" id="777">
				<include>rightSideSlide</include>
				<!--BG Dim-->		
				<control type="group">
					<visible>Control.IsVisible(54)</visible>
					<visible>!Skin.String(54_Dim,100)</visible>
					<control type="group">
						<visible>Skin.String(54_Dim,10)</visible>
						<include>BGDim10</include>
					</control>
					<control type="group">
						<visible>Skin.String(54_Dim,20)</visible>
						<include>BGDim20</include>
					</control>
					<control type="group">
						<visible>Skin.String(54_Dim,30)</visible>
						<include>BGDim30</include>
					</control>
					<control type="group">
						<visible>Skin.String(54_Dim,40)</visible>
						<include>BGDim40</include>
					</control>
					<control type="group">
						<visible>Skin.String(54_Dim,50)</visible>
						<include>BGDim50</include>
					</control>
					<control type="group">
						<visible>Skin.String(54_Dim,60) | IsEmpty(Skin.String(54_Dim))</visible>
						<include>BGDim60</include>
					</control>
					<control type="group">
						<visible>Skin.String(54_Dim,70)</visible>
						<include>BGDim70</include>
					</control>
					<control type="group">
						<visible>Skin.String(54_Dim,80)</visible>
						<include>BGDim80</include>
					</control>
					<control type="group">
						<visible>Skin.String(54_Dim,90)</visible>
						<include>BGDim90</include>
					</control>
				</control>
				<control type="panel" id="54">
					<viewtype label="Shared Content">panel</viewtype>
					<onup>PreviousMenu</onup>
					<ondown>54</ondown>
					<onright>54</onright>
					<onleft>998</onleft>
					<orientation>vertical</orientation>
					<posx>52</posx>
					<posy>38</posy>
					<width>1220</width>
					<height>645</height>
					<itemlayout width="304" height="320">
						<include>SharedContentLeafs</include>
						<control type="label">
							<include>SharedContentLeafsTitle</include>
							<textcolor>Unfocused</textcolor>
						</control>
						<control type="label">
							<include>SharedContentLeafsTitle2</include>
						</control>
					</itemlayout>
					<focusedlayout width="304" height="320">
						<include>SharedContentLeafs</include>
						<control type="label">
							<include>SharedContentLeafsTitle</include>
							<textcolor>Focused</textcolor>
						</control>
						<control type="label">
							<include>SharedContentLeafsTitle2</include>
						</control>
						<include>SharedContentLeafsSelectedBorder</include>
					</focusedlayout>
				</control>
			</control>
		</control>
	</include>
</includes>
